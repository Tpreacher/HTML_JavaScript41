
//example of lines
ctx.moveTo(0,0);
ctx.lineTo(800,600);
ctx.stroke();

ctx.moveTo(800,0);
ctx.lineTo(0,600);
ctx.stroke();


ctx.fillStyle = 'purple';
ctx.fillRect(c.width/4,c.height/4 ,c.width/2,c.height/2);

//Draw Circle
ctx.strokeStyle = 'red';
ctx.fillStyle = 'orange';
ctx.beginPath();
ctx.arc(c.width/2, c.height/2, 50, 0, 2*Math.PI, false);
ctx.fill();
ctx.stroke();